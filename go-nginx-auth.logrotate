/var/log/go-nginx-auth/go-nginx-auth_http.log {
    daily
    rotate 365
    copytruncate
    delaycompress
    compress
    notifempty
    missingok
}

/var/log/go-nginx-auth/go-nginx-auth_app.log {
    daily
    rotate 365
    copytruncate
    delaycompress
    compress
    notifempty
    missingok
}
